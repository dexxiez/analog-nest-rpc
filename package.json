{
  "name": "analog-nest-rpc",
  "version": "0.0.1",
  "description": "POC Compiler-driven RPC bridge between AnalogJS and NestJS",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "import": "./dist/decorators.js"
    },
    "./h3": {
      "types": "./dist/h3/index.d.ts",
      "import": "./dist/h3/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "analogjs",
    "nestjs",
    "rpc",
    "vite",
    "angular"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "h3": ">=1.0.0",
    "reflect-metadata": ">=0.1.0",
    "superjson": ">=2.0.0",
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "h3": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "glob": "^13.0.0",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^22.0.0",
    "h3": "^1.13.0",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "superjson": "^2.2.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.7"
  }
}
